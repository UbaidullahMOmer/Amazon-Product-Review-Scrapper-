{
    "manifest_version": 2,
    "name": "Company Logo Downloader",
    "version": "1.0",
    "description": "Downloads all company logos from the page",
    "permissions": [
      "activeTab",
      "downloads"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }